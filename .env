# Configurações do Sistema NFe - DESENVOLVIMENTO

# Ambiente SEFAZ (1=Produção, 2=Homologação)
AMBIENTE=2

# UF do emitente (código IBGE)
UF_CODIGO=50
UF_SIGLA=MS

# Certificado Digital A1 (PFX) - CONFIGURAR CAMINHO REAL
CERT_PATH=./certificados/certificado.pfx
CERT_PASSWORD=senha_certificado

# Dados do Emitente
EMITENTE_CNPJ=
EMITENTE_RAZAO_SOCIAL=
EMITENTE_NOME_FANTASIA=
EMITENTE_IE=283469151
EMITENTE_IM=
EMITENTE_CNAE=6201500
EMITENTE_CRT=3

# Endereço do Emitente
EMITENTE_LOGRADOURO=
EMITENTE_NUMERO=123
EMITENTE_COMPLEMENTO=
EMITENTE_BAIRRO=CENTRO
EMITENTE_CEP=79002000
EMITENTE_MUNICIPIO=CAMPO GRANDE
EMITENTE_UF=MS
EMITENTE_PAIS=1058
EMITENTE_TELEFONE=6733334444

# Configurações de Sistema
SIMULATION_MODE=true
SAVE_XML_FILES=true
LOG_LEVEL=debug

# Timeouts (em milissegundos)
REQUEST_TIMEOUT=30000
SOAP_TIMEOUT=60000

# Configurações de SSL
SSL_VERIFY=false
SSL_REJECT_UNAUTHORIZED=false

# Diretórios
XML_OUTPUT_DIR=./xmls
CERT_DIR=./certificados
LOG_DIR=./logs