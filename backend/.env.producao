# Configurações para PRODUÇÃO
PORT=3001
NODE_ENV=production

# Autenticação
JWT_SECRET=prod-secret-key-brandao-contador-nfe-2024-CHANGE-THIS
JWT_EXPIRES_IN=24h
API_KEYS=nfe-prod-key-CHANGE-THIS,nfe-prod-key-2-CHANGE-THIS

# NFe/SEFAZ - PRODUÇÃO
AMBIENTE=1
UF=SVRS
CNPJ_EMITENTE=

# Certificado
CERT_PATH=
CERT_PASS=1234

# Logs
LOGS_DIR=./logs
LOG_LEVEL=warn

# Rede
TIMEOUT=30000
RETRY_ATTEMPTS=3
KEEP_ALIVE=true

# Segurança
RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW=900000
CORS_ORIGINS=https://brandaocontador.com.br,https://app.brandaocontador.com.br

# NFe específicas
NFE_SERIE_PADRAO=1
NFE_NUMERO_INICIAL=1

# Produção
DEBUG_MODE=false
SIMULATION_MODE=false
SAVE_XML_FILES=true
XML_OUTPUT_DIR=./xmls